<?php

/**
 * @file
 * Install file for lesson_rules module.
 */

/**
 * Implements hook_schema().
 */
function lesson_rules_schema() {
  $schema['cache_lesson_rules'] = drupal_get_schema_unprocessed(
    'system',
    'cache'
  );
  $schema['cache_lesson_rules']['description'] = 'Lottery cache';

  return $schema;
}
